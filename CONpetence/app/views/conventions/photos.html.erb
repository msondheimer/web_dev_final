<p> <%= link_to "Add a photo", "/conventions/#{@con.id}/photos/add", class: 'btn btn-success pull-center add' %></p>

<div>
	<% @photos.each do |photo| %>
    	<div class="dan-col-md-4 dan-chart dan-well">
        	<%= link_to image_tag(photo.photo_url), "/photos/#{photo.id}" %>
    	</div>
	<% end %>
</div>

